//
//  TwilioSDKHost-Config.xcconfig
//
//

SDKROOT = iphoneos

ARCHS = i386 armv7
ARCHS[sdk=iphoneos*] = armv7


IPHONEOS_DEPLOYMENT_TARGET = 6.0

INFOPLIST_FILE = TwilioCoreSDKHost/TwilioCoreSDKHost-Info.plist
GCC_PRECOMPILE_PREFIX_HEADER = YES
GCC_PREFIX_HEADER = TwilioCoreSDKHost/TwilioCoreSDKHost-Prefix.pch

// Compiler - Language.
GCC_C_LANGUAGE_STANDARD = c99
PREBINDING = NO

// Compiler - Language - C++.
CLANG_CXX_LANGUAGE_STANDARD = c++0x
CLANG_CXX_LIBRARY = libstdc++

GCC_OPTIMIZATION_LEVEL = 0
GCC_SYMBOLS_PRIVATE_EXTERN = NO

// Compiler - Language - Modules.
CLANG_ENABLE_MODULES = YES

OTHER_LDFLAGS = $(inherited) -ObjC

TWILIO_SDK_HEADERS_DIR = $(SRCROOT)/TwilioCoreSDK/Sources/Core

HEADER_SEARCH_PATHS = $(inherited) $(TWILIO_SDK_HEADERS_DIR)

LIBRARY_SEARCH_PATHS = $(inherited)
